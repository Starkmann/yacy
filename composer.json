{
	"name": "eike/yacy",
	"type": "typo3-cms-extension",
	"description": "Integrate yacy",
	"license": [
		"GPL-2.0+"
	],
	"keywords": [
		"integration",
		"yacy",
		"search"
	],
	"support": {
		"issues": "https://github.com/Starkmann/yacy/issues"
	},
	"require": {
		"typo3/cms-core": "^11.5"
	},
	"replace": {
		"typo3-ter/yacy": "self.version"
	},
	"autoload": {
		"psr-4": {
			"Eike\\Yacy\\": "Classes"
		}
	},
	"extra": {
      "typo3/cms": {
         "extension-key": "yacy"
      }
   }
}
